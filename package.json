{
  "name": "@alicloud/open-drive",
  "version": "0.1.0",
  "description": "Awesome data dispatcher system, cloud drive system.",
  "license": "MIT",
  "author": {
    "name": "aleelock"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/aliyun/open-drive.git"
  },
  "bin": {
    "opendrive": "./bin/cli.js"
  },
  "scripts": {
    "start": "node bin/www",
    "dev": "npm-run-all --parallel dev:*",
    "dev:sv": "cross-env NODE_ENV=development DEBUG=ots,oss,svs*  ./node_modules/.bin/nodemon bin/www",
    "dev:ui": "cd ui && npm run serve",
    "build": "cd ui && npm run build",
    "clean": "shx rm -rf node_modules ui/node_modules dist",
    "prd": "pm2 start bin/www",
    "test": "mocha test"
  },
  "dependencies": {
    "@alicloud/fc2": "^2.0.0",
    "@alicloud/pop-core": "^1.7.0",
    "ali-oss": "^6.0.1",
    "alipay-sdk": "^3.0.4",
    "axios": "^0.18.0",
    "buffer": "4.9.1",
    "commander": "^2.19.0",
    "debug": "^2.6.3",
    "ejs": "~2.3.3",
    "int64-buffer": "0.1.9",
    "jsonwebtoken": "^8.4.0",
    "jwk-to-pem": "^2.0.0",
    "koa": "^2.2.0",
    "koa-bodyparser": "^3.2.0",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^2.0.1",
    "koa-onerror": "^1.2.1",
    "koa-router": "^7.1.1",
    "koa-static": "^3.0.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "node-jose": "^1.1.0",
    "path-to-regexp": "^2.1.0",
    "protobufjs": "^4.1.2",
    "tablestore": "^4.0.14",
    "underscore": "^1.9.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "nodemon": "^1.8.1",
    "npm-run-all": "^4.1.3",
    "shx": "^0.3.2"
  }
}
